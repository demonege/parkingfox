<?xml version='1.0' encoding='utf-8'?>
<widget id="Parkingfox" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Parkingfox</name>
    <description>Parkingfox is real</description>
    <author email="amnael.worldwalker@web.de" href="">Rencher Dev team</author>
    <content src="www/index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="orientation" value="portrait" />
	<platform name="ios" />
	<platform name="android" />
	<platform name="winphone" />
</widget>
